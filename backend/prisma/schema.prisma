datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ReversedIP {
  id         String   @id @default(uuid())
  originalIP String
  reversedIP String
  requestIP  String
  userAgent  String?
  createdAt  DateTime @default(now())
  timestamp  DateTime @default(now())
}